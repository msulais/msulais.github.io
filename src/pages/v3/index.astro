---
import Html from "../../layouts/Html.astro";
import VersionWarning from '../../components/VersionWarning/index.astro'
import './_styles/variables.scss'
import './_styles/index.scss'
import './_styles/block-effect.scss'
import './_styles/footer.scss'
import './_styles/header.scss'
import './_styles/language.scss'
import './_styles/logo.scss'
import './_styles/main.scss'
import './_styles/theme.scss'
import './_styles/pages/index.scss'
import './_styles/pages/about.scss'
import './_styles/pages/contact.scss'
import './_styles/pages/works.scss'

import favicon_ico from './_favicon/favicon.ico'
import favicon_appletouchicon from './_favicon/apple-touch-icon.png'
import favicon_32 from './_favicon/favicon-32x32.png'
import favicon_16 from './_favicon/favicon-16x16.png'
import { C_Contacts } from "../../constants/contact";
import { C_Quotes } from "../../constants/quotes";
import { C_About } from "../../constants/about";
---

<Html data-theme="system">
	<Fragment slot="favicon">
		<link rel="icon" href={favicon_ico} />
		<link rel="shortcut icon" type="image/ico" href={favicon_ico} />
		<link rel="apple-touch-icon" sizes="180x180" href={favicon_appletouchicon.src}>
		<link rel="icon" type="image/png" sizes="32x32" href={favicon_32.src}>
		<link rel="icon" type="image/png" sizes="16x16" href={favicon_16.src}>
	</Fragment>
	<div id="g-route-listener"></div>
    <header>
        <button id="h-logo-btn">
            <div id="logo">
                <div></div>
                <div></div>
                <div></div>
            </div>
            <span>Muhammad&nbsp;</span>Sulais
        </button>
        <nav id="h-navigation">
            <a href="#" data-selected>
                <span lang="en">Home</span>
            </a>
            /
            <a href="#about">
                <span lang="en">About</span>
            </a>
            /
            <a href="#works">
                <span lang="en">Works</span>
            </a>
            /
            <a href="#contact">
                <span lang="en">Contact</span>
            </a>
        </nav>
        <button id="h-menu">
            <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))">
                <path d="M120-240v-80h720v80H120Zm0-200v-80h720v80H120Zm0-200v-80h720v80H120Z"/>
            </svg>
        </button>
        <dialog id="h-dialog">
            <button id="h-dialog-close">
                <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))"><path d="m256-200-56-56 224-224-224-224 56-56 224 224 224-224 56 56-224 224 224 224-56 56-224-224-224 224Z"/></svg>
            </button>
            <a href="#" data-selected>
                <span lang="en">Home</span>
            </a>
            <a href="#about">
                <span lang="en">About</span>
            </a>
            <a href="#works">
                <span lang="en">Works</span>
            </a>
            <a href="#contact">
                <span lang="en">Contact</span>
            </a>
        </dialog>
    </header>
    <main class="no-view">
        <div data-page-target="#" data-selected>
            <h1>
                <span lang="en">{C_Quotes.HeroTitle}</span>
            </h1>
            <p>
                <span lang="en">{C_Quotes.HeroSubtitle}</span>
            </p>
            <button id="mp-home-btn">
                <span data-state="1">{C_About.role}</span>
                <span data-state="2">
                    <span lang="en">Contact me</span>
                </span>
            </button>
        </div>
        <div data-page-target="#about">
            <h2>
                <span lang="en">About Me</span>
            </h2>
            {C_About.intro.map(v =>
                <p>{v}</p>
            )}
            <details open>
                <summary>
                    <span lang="en">Skills</span>
                    <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))"><path d="M440-313v-447q0-17 11.5-28.5T480-800q17 0 28.5 11.5T520-760v447l196-196q12-12 28-11.5t28 12.5q11 12 11.5 28T772-452L508-188q-6 6-13 8.5t-15 2.5q-8 0-15-2.5t-13-8.5L188-452q-11-11-11-27.5t11-28.5q12-12 28.5-12t28.5 12l195 195Z"/></svg>
                </summary>
                <div>
                    <ul class="mp-about-skills">
                        {C_About.Skills.all.map(v =>
                            <li><img src={v.imgURL}/>{v.name}</li>
                        )}
                    </ul>
                </div>
            </details>
            <details open>
                <summary>
                    <span lang="en">Educations</span>
                    <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))"><path d="M440-313v-447q0-17 11.5-28.5T480-800q17 0 28.5 11.5T520-760v447l196-196q12-12 28-11.5t28 12.5q11 12 11.5 28T772-452L508-188q-6 6-13 8.5t-15 2.5q-8 0-15-2.5t-13-8.5L188-452q-11-11-11-27.5t11-28.5q12-12 28.5-12t28.5 12l195 195Z"/></svg>
                </summary>
                <div>
                    <ul class="mp-about-educations">
                        {C_About.Educations.all.map(v =>
                            <li>{v.university}<ul>
                                <li>{v.degree} ({v.from_year} - {v.to_year})</li>
                            </ul></li>
                        )}
                    </ul>
                </div>
            </details>
            <details open>
                <summary>
                    <span lang="en">Work history</span>
                    <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))"><path d="M440-313v-447q0-17 11.5-28.5T480-800q17 0 28.5 11.5T520-760v447l196-196q12-12 28-11.5t28 12.5q11 12 11.5 28T772-452L508-188q-6 6-13 8.5t-15 2.5q-8 0-15-2.5t-13-8.5L188-452q-11-11-11-27.5t11-28.5q12-12 28.5-12t28.5 12l195 195Z"/></svg>
                </summary>
                <div>
                    <ul class="mp-about-works">
                        {C_About.Works.all.map(v =>
                            <li>{v.company}<ul>
                                <li>{v.role} ({v.from_year} - {v.to_year < 0? 'Present' : v.to_year})</li>
                            </ul></li>
                        )}
                    </ul>
                </div>
            </details>
            <details open>
                <summary>
                    <span lang="en">Social media</span>
                    <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="rgb(var(--g-color-on-surface))"><path d="M440-313v-447q0-17 11.5-28.5T480-800q17 0 28.5 11.5T520-760v447l196-196q12-12 28-11.5t28 12.5q11 12 11.5 28T772-452L508-188q-6 6-13 8.5t-15 2.5q-8 0-15-2.5t-13-8.5L188-452q-11-11-11-27.5t11-28.5q12-12 28.5-12t28.5 12l195 195Z"/></svg>
